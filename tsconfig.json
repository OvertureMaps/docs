{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"],
      "@docusaurus/*": ["node_modules/@docusaurus/*"]
    }
  },
  "include": ["src/**/*", "docs/**/*", "*.js", "*.ts"],
  "exclude": ["node_modules", "build", ".docusaurus"]
}
